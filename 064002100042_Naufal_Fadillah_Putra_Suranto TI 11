import math
print("cari koordinat")
x1 = eval(input("masukan nilai x1= "))
x2 = eval(input("masukan nilai x2= "))
y1 = eval(input("masukan nilai y1= "))
y2 = eval(input("masukan nilai y2= "))

print("hasilnya adalah:",math.sqrt((x1-x2)**2+(y1-y2)))

